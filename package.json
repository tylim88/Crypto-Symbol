{
	"name": "crypto-symbol",
	"version": "2.0.1",
	"description": "Provide easy conversion between crypto symbol and name",
	"main": "dist/index.js",
	"types": "types/index.d.ts",
	"scripts": {
		"reinstall": "rm -rf node_modules/ && rm -rf package-lock.json && npm i",
		"tsc": "tsc -p tsconfig.json && npm run dummy",
		"lint": "eslint ./**/*.ts ",
		"lint-fix": "eslint ./**/*.ts --fix",
		"build": "rm -rf dist && rm -rf types && babel src --out-dir dist --extensions \".ts\" && tsc -p tsconfig.json",
		"test": "jest",
		"pub": "npm run build && npm publish",
		"dummy": ""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tylim88/Crypto-Symbol.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tylim88/Crypto-Symbol/issues"
	},
	"homepage": "https://github.com/tylim88/Crypto-Symbol#readme",
	"keywords": [
		"crypto",
		"name",
		"symbol",
		"Bitcoin",
		"Ethereum",
		"cryptoCurrency",
		"currency",
		"blockchain",
		"converter"
	],
	"devDependencies": {
		"@babel/cli": "^7.4.3",
		"@babel/core": "^7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-export-namespace-from": "^7.14.5",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-typescript": "^7.10.4",
		"@types/jest": "^27.0.2",
		"@typescript-eslint/eslint-plugin": "^3.7.0",
		"@typescript-eslint/parser": "^3.7.0",
		"babel-core": "^6.26.3",
		"babel-jest": "^27.2.5",
		"babel-plugin-module-resolver": "^4.1.0",
		"eslint": "^7.5.0",
		"eslint-cli": "^1.1.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.2.5",
		"jest": "^27.2.5",
		"jsdoc": "^3.6.5",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"regenerator-runtime": "^0.13.9",
		"ts-jest": "^27.0.5",
		"typescript": "^3.9.7"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run tsc && lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts}": [
			"npm run lint --fix",
			"prettier --write"
		]
	}
}
